{
  "name": "kkv-stockholm",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbo & pnpm run codegen",
    "start": "next start",
    "preview": "pnpm run build && pnpm run start",
    "build": "next build",
    "prebuild": "tsx ./auth/init.ts",
    "dev:email": "email dev --port 4000",
    "codegen": "pnpm run codegen:datocms & pnpm run codegen:graphql",
    "codegen:graphql": "graphql-codegen --watch",
    "codegen:datocms": "datocms schema:generate ./types/datocms.ts",
    "lint": "next lint",
    "reset": "rm -rf ./node_modules && rm -rf pnpm-lock.yaml && rm -rf .next && pnpm i",
    "reset:cache": "rm -rf .next/cache"
  },
